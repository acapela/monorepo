  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.19.0",
    "@sentry/node": "^6.19.7",
    "koa": "^2.13.4"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "esbuild": "^0.14.39",
    "ts-node-dev": "^1.1.8"
  }
}
