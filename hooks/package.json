{
  "name": "@aca/hooks",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only -- ./src/index.ts",
    "start": "./scripts/start.sh",
    "build": "esbuild ./src/index.ts --outdir=dist --target=node16 --bundle --platform=node --sourcemap",
