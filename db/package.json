{
  "name": "~db",
  "version": "0.1.0",
  "scripts": {
    "dbenv": "dotenv -e ../.env",
    "introspect": "yarn dbenv prisma introspect",
    "validate": "yarn dbenv prisma validate",
    "generate": "yarn dbenv prisma generate",
    "update": "yarn introspect && yarn generate",
    "postinstall": "yarn generate"
  },
  "devDependencies": {
    "cross-env": "7.0.3"
  },
  "dependencies": {
    "@prisma/client": "^2.17.0",
    "dotenv-cli": "^4.0.0",
    "prisma": "^2.17.0",
    "~config": "0.1.0",
    "~shared": "0.1.0"
  }
}
