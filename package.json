{
  "name": "playground",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest src --runInBand",
    "test:watch": "jest src --runInBand --watch",
    "test:hasura": "jest hasura/tests --runInBand",
    "test:hasura:watch": "jest hasura/tests --runInBand --watch",
    "build": "tsc",
    "start": "node dist",
    "dev": "tsc-watch --onSuccess 'node ./dist'",
    "dev-hasura": "hasura console --project hasura --admin-secret dev",
    "format": "prettier --write src",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "checkformat": "prettier --check src",
    "gcp-build": "NODE_ENV=production npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run format"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.8",
    "@types/pino": "^6.3.3",
    "@types/pino-http": "^5.0.5",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.4.1",
    "@google-cloud/secret-manager": "^3.2.2",
    "@sendgrid/mail": "^7.4.0",
    "axios": "^0.19.2",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^9.4.1",
    "hasura-cli": "^1.3.3",
    "helmet": "^4.2.0",
    "js-yaml": "^3.14.0",
    "knex": "^0.21.12",
    "lodash": "^4.17.20",
    "pg": "^8.5.1",
    "pino": "^6.7.0",
    "uuid": "^8.3.1"
  }
}
