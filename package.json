{
  "name": "playground",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest src --runInBand",
    "test:watch": "jest src --runInBand --watch",
    "build": "tsc",
    "start": "node dist",
    "format": "prettier --write --single-quote src",
    "checkformat": "prettier --check src"
  },
  "husky": {
