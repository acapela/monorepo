    "@types/pino": "^6.3.0",
    "@types/pino-http": "^5.0.5",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "pino-pretty": "^4.2.1",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.4.1",
    "config": "^3.3.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^9.2.0",
