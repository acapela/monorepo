{
  "name": "~ui",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "peerDependencies": {
    "react": "*",
    "styled-components": "*"
  },
  "scripts": {
    "icons:clear": "rimraf icons/fill icons/outline",
    "icons:generate:fill": "svgr --config-file .icons.yml -d icons/fill ../node_modules/eva-icons/fill/svg",
    "icons:generate:outline": "svgr --config-file .icons.yml -d icons/outline ../node_modules/eva-icons/outline/svg",
    "icons:generate": "yarn icons:clear && yarn icons:generate:fill && yarn icons:generate:outline"
  },
  "dependencies": {
    "emoji-mart": "^3.0.1",
    "eva-icons": "^1.1.3",
    "proxy-deep": "^3.1.1",
    "react-virtualized": "^9.22.3",
    "~shared": "0.1.0"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/emoji-mart": "^3.0.6",
    "@types/react-virtualized": "^9.21.13"
  }
}
